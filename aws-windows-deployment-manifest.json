{
    "manifestVersion": 1,
    "iisConfig": {
        "appPools": [
            {
                "name": "customPool",
                "recycling": {
                    "regularTimeInterval": 1
                }
            }
        ]
    },
    "deployments": {
        "aspNetCoreWeb": [
            {
                "name": "test-dotnet-core",
                "parameters": {
                    "appBundle": "site.zip",
                    "iisPath": "/",
